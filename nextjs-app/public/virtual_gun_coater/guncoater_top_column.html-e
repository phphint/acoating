
<script language="javascript">
  function changeCoatTop(title, new_image, type) {
      if(type == 'top') {
          image = parent.document.top_image;
          image_title = parent.document.gcForm.top_image_title;
          image_path = parent.document.gcForm.top_image_file;
      }
      if(type == 'bottom') {
          image = parent.document.bottom_image;
          image_title = parent.document.gcForm.bottom_image_title;
          image_path = parent.document.gcForm.bottom_image_file;
      }
      
      image.src=new_image;
      image.alt=title;
      image.title=title;
  
      image_title.value=title;
      image_path.value=new_image;
  
  }
  function changeBg() {
      this.className='gcSelected';
  }
  </script>
